<?xml version="1.0"?>
<configuration>
  <system.webServer>
    <modules>
      <add name="RpcModule" type="IntelliFactory.WebSharper.Web.RpcModule, IntelliFactory.WebSharper.Web"/>
    </modules>
  </system.webServer>
  <system.web>
    <httpModules>
      <add name="RpcModule" type="IntelliFactory.WebSharper.Web.RpcModule, IntelliFactory.WebSharper.Web"/>
    </httpModules>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID">
      <controls>
        <add tagPrefix="WebSharper" namespace="IntelliFactory.WebSharper.Web" assembly="IntelliFactory.WebSharper.Web"/>
        <add tagPrefix="Tests" namespace="IntelliFactory.WebSharper.JQuery.Mobile.Tests" assembly="IntelliFactory.WebSharper.JQuery.Mobile.Tests"/>
      </controls>
    </pages>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>
</configuration>
